	<queryString language="SQL">
		<![CDATA[SELECT id_taxista as 'CÓDIGO',CONCAT(nombre," ",apellido) AS 'DATOS PERSONALES',SUM(num_persona) as 'CANTIDAD DE PERSONAS', telefono AS 'TELÉFONO'
FROM taxista t INNER JOIN recomienda r on r.taxista_id_taxista=t.id_taxista
GROUP BY nombre,apellido,id_taxista
ORDER BY 3 DESC]]>
